<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/ai_start_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="16dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/ai_start_guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintGuide_begin="205dp"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/ai_start_guideline_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="16dp"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ai_farm_farmer_name_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Farm/Farmer name"
        android:textColor="@color/default_color"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline"
        app:layout_constraintTop_toTopOf="@+id/ai_start_guideline_horizontal" />

    <EditText
        android:id="@+id/ai_farm_farmer_name_et"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/spinner_boarder"
        android:textColor="@color/default_color"
        app:layout_constraintBaseline_toBaselineOf="@+id/ai_farm_farmer_name_tv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/ai_farm_farmer_name_tv" />

    <TextView
        android:id="@+id/ai_eartag_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="8dp"
        android:text="Ear Tag No:"
        android:textColor="@color/default_color"
        android:textSize="20dp"
        app:layout_constraintEnd_toStartOf="@+id/ai_start_guideline2"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline"
        app:layout_constraintTop_toBottomOf="@+id/ai_farm_farmer_name_tv" />

    <EditText
        android:id="@+id/ai_eartag_et"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@drawable/spinner_boarder"
        android:textColor="@color/default_color"
        app:layout_constraintEnd_toEndOf="@+id/ai_eartag_tv"
        app:layout_constraintStart_toStartOf="@+id/ai_eartag_tv"
        app:layout_constraintTop_toBottomOf="@+id/ai_eartag_tv" />

    <TextView
        android:id="@+id/ai_animalname_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="Animal Name"
        android:textColor="@color/default_color"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/ai_eartag_tv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline2"
        app:layout_constraintTop_toTopOf="@+id/ai_eartag_tv"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/ai_animalname_et"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@drawable/spinner_boarder"
        android:textColor="@color/default_color"
        app:layout_constraintEnd_toEndOf="@+id/ai_animalname_tv"
        app:layout_constraintStart_toStartOf="@+id/ai_animalname_tv"
        app:layout_constraintTop_toBottomOf="@+id/ai_animalname_tv" />

    <TextView
        android:id="@+id/ai_mobile_no_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Mobile No"
        android:textColor="@color/default_color"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline"
        app:layout_constraintTop_toBottomOf="@+id/ai_eartag_et" />

    <EditText
        android:id="@+id/ai_mobile_no_et"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:background="@drawable/spinner_boarder"
        android:textColor="@color/default_color"
        app:layout_constraintBaseline_toBaselineOf="@+id/ai_mobile_no_tv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.929"
        app:layout_constraintStart_toEndOf="@+id/ai_mobile_no_tv" />

    <Spinner
        android:id="@+id/ai_animal_species_spinner_input"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/spinner_boarder_with_icon"
        app:layout_constraintEnd_toStartOf="@+id/ai_start_guideline2"
        app:layout_constraintStart_toStartOf="@+id/ai_animal_species_tv"
        app:layout_constraintTop_toBottomOf="@+id/ai_animal_species_tv" />

    <TextView
        android:id="@+id/ai_animal_species_tv"
        android:layout_width="0dp"
        android:layout_height="36dp"
        android:layout_marginTop="24dp"
        android:text="Species"
        android:textColor="@color/default_color"
        app:layout_constraintEnd_toEndOf="@+id/ai_eartag_et"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline"
        app:layout_constraintTop_toBottomOf="@+id/ai_mobile_no_tv" />

    <Spinner
        android:id="@+id/ai_animal_breed_spinner_input"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/spinner_boarder_with_icon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.058"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline2"
        app:layout_constraintTop_toBottomOf="@+id/ai_animal_breed_tv" />

    <TextView
        android:id="@+id/ai_animal_breed_tv"
        android:layout_width="0dp"
        android:layout_height="36dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:text="Breed"
        android:textColor="@color/default_color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline2"
        app:layout_constraintTop_toBottomOf="@+id/ai_mobile_no_et" />

    <TextView
        android:id="@+id/ai_date_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="AI Date"
        android:textColor="@color/default_color"
        android:textSize="20dp"
        app:layout_constraintEnd_toStartOf="@+id/ai_start_guideline2"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline"
        app:layout_constraintTop_toBottomOf="@+id/ai_animal_species_spinner_input" />

    <EditText
        android:id="@+id/ai_date_et"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/spinner_boarder"
        android:textColor="@color/default_color"
        app:layout_constraintEnd_toStartOf="@+id/ai_start_guideline2"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline"
        app:layout_constraintTop_toBottomOf="@+id/ai_date_tv" />

    <TextView
        android:id="@+id/ai_repeat_tv"
        android:layout_width="0dp"
        android:layout_height="36dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:text="Repeat AI"
        android:textColor="@color/default_color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline2"
        app:layout_constraintTop_toBottomOf="@+id/ai_animal_breed_spinner_input" />

    <Spinner
        android:id="@+id/ai_repeat_ai_spinner_input"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="7dp"
        android:background="@drawable/spinner_boarder_with_icon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline2"
        app:layout_constraintTop_toBottomOf="@+id/ai_repeat_tv" />

    <TextView
        android:id="@+id/ai_semen_bull_id_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:text="Semen Bull ID"
        android:textColor="@color/default_color"
        android:textSize="20dp"
        app:layout_constraintEnd_toStartOf="@+id/ai_start_guideline2"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline"
        app:layout_constraintTop_toBottomOf="@+id/ai_date_et" />

    <EditText
        android:id="@+id/ai_semen_bull_id_et"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/spinner_boarder"
        android:textColor="@color/default_color"
        app:layout_constraintEnd_toStartOf="@+id/ai_start_guideline2"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline"
        app:layout_constraintTop_toBottomOf="@+id/ai_semen_bull_id_tv" />

    <TextView
        android:id="@+id/ai_semen_number_tv"
        android:layout_width="181dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Semen Number"
        android:textColor="@color/default_color"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline2"
        app:layout_constraintTop_toBottomOf="@+id/ai_repeat_ai_spinner_input" />

    <EditText
        android:id="@+id/ai_semen_number_et"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/spinner_boarder"
        android:textColor="@color/default_color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline2"
        app:layout_constraintTop_toBottomOf="@+id/ai_semen_number_tv" />

    <TextView
        android:id="@+id/ai_fiscal_year_tv"
        android:layout_width="0dp"
        android:layout_height="36dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:text="Fiscal Year"
        android:textColor="@color/default_color"
        app:layout_constraintEnd_toStartOf="@+id/ai_start_guideline2"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline"
        app:layout_constraintTop_toBottomOf="@+id/ai_semen_bull_id_et" />

    <Spinner
        android:id="@+id/ai_fiscal_year_input"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/spinner_boarder_with_icon"
        app:layout_constraintEnd_toStartOf="@+id/ai_start_guideline2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline"
        app:layout_constraintTop_toBottomOf="@+id/ai_fiscal_year_tv" />

    <TextView
        android:id="@+id/ai_technician_name_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:text="Technician Name"
        android:textColor="@color/default_color"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline2"
        app:layout_constraintTop_toBottomOf="@+id/ai_semen_number_et" />

    <EditText
        android:id="@+id/ai_technician_name_et"
        android:layout_width="0dp"
        android:layout_height="38dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/spinner_boarder"
        android:textColor="@color/default_color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline2"
        app:layout_constraintTop_toBottomOf="@+id/ai_technician_name_tv" />

    <Button
        android:id="@+id/register_ai"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Submit"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/ai_start_guideline"
        app:layout_constraintTop_toBottomOf="@+id/ai_fiscal_year_input" />

</androidx.constraintlayout.widget.ConstraintLayout>